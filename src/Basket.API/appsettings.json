{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost",
    "EventBus": "amqp://localhost"
  },
  "Identity": {
    "Audience": "basket"
  },
  "EventBus": {
    "SubscriptionClientName": "Basket"
  },
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://host.docker.internal:4316",
  "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
  "OTEL_RESOURCE_ATTRIBUTES": "service.name=basket-api"
}
